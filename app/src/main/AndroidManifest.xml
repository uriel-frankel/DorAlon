<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.comrax.doralonapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.comrax.doralonapp.DorAlonApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.comrax.doralonapp.MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.comrax.doralonapp.StationsListActivity"/>
        <activity android:name="com.comrax.doralonapp.SalesListActivity"/>
        <activity android:name="com.comrax.doralonapp.AgreementActivity"/>
        <activity android:name="com.comrax.doralonapp.CoffeeActivity"/>
        <activity android:name="com.comrax.doralonapp.StationActivity"/>

    </application>

</manifest>